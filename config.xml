<?xml version='1.0' encoding='utf-8'?>
<widget id="com.launcher.app" version="1.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Launcher App</name>
    <description>
        Hello World sample application that responds to the deviceready event.
    </description>
    <author email="support@phonegap.com" href="http://phonegap.com">
        PhoneGap Team
    </author>
    <content src="index.html" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" /> 
    <platform name="android">
        <icon density="ldpi" src="www/res/icon/android/android-icon-36x36.png" />
        <icon density="mdpi" src="www/res/icon/android/android-icon-48x48.png" />
        <icon density="hdpi" src="www/res/icon/android/android-icon-72x72.png" />
        <icon density="xhdpi" src="www/res/icon/android/android-icon-96x96.png" />
        <icon density="xxhdpi" src="www/res/icon/android/android-icon-144x144.png" />
        <icon density="xxxhdpi" src="www/res/icon/android/android-icon-192x192.png" /> 
    </platform> 
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <access origin="whatsapp:*" launch-external="yes" />
    <plugin name="cordova-plugin-whitelist" spec="~1.3.4" />
</widget>
